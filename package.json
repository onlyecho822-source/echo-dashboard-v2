{
  "name": "echo-dashboard-v2",
  "version": "2.0.0",
  "description": "Institutional Epistemic Infrastructure - Counter-Intelligence-Grade Cognitive Capture Detection",
  "type": "module",
  "license": "MIT",
  "author": "Echo Project",
  "repository": {
    "type": "git",
    "url": "https://github.com/onlyecho822-source/echo-dashboard-v2.git"
  },
  "bugs": {
    "url": "https://github.com/onlyecho822-source/echo-dashboard-v2/issues"
  },
  "homepage": "https://github.com/onlyecho822-source/echo-dashboard-v2#readme",
  "keywords": [
    "epistemic-infrastructure",
    "institutional-monitoring",
    "cognitive-capture",
    "governance-resilience",
    "defense-in-depth",
    "institutional-economics"
  ],
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "concurrently \"pnpm:dev:api\" \"pnpm:dev:frontend\"",
    "dev:api": "NODE_ENV=development tsx watch src/api/server.ts",
    "dev:frontend": "vite",
    "build": "pnpm build:api && pnpm build:frontend",
    "build:api": "tsc -p tsconfig.api.json && esbuild src/api/server.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/api",
    "build:frontend": "vite build",
    "start": "NODE_ENV=production node dist/api/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "db:push": "drizzle-kit generate && drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "health-check": "./scripts/health-check.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@aws-sdk/s3-request-presigner": "^3.693.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.15",
    "@radix-ui/react-select": "^2.1.15",
    "@radix-ui/react-slot": "^1.1.15",
    "@radix-ui/react-tabs": "^1.1.15",
    "@radix-ui/react-toast": "^1.2.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.38.3",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.468.0",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.3",
    "wouter": "^3.5.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.30.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
